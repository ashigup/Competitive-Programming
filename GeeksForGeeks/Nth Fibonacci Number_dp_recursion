//Nth fibbonacci number-----geeksforgeeks----------dynamic programming
//Author -- ashigup


#include<iostream>
#define ll long long int
#define m 1000000007
using namespace std;
ll dp[1005];
ll fib(ll n)
{
    if(n==0 || n==1)
      return n;
      
    if(dp[n]!=-1)
      return dp[n];
    else
     {
         dp[n]=(fib(n-1)%m+fib(n-2)%m)%m;
         return dp[n];
     }
    
}


int main()
{ 
  ll n,t;
  
  cin>>t;
  while(t--){
  cin>>n;
  for(ll i=0;i<=n;i++)
    dp[i]=-1;
  cout<<fib(n)<<"\n";
  }
    return 0;
}
